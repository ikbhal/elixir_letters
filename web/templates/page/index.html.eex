<div id="fridge"></div>

<script>
  function get_letters() {
    return [
      <%= for {<< letter_code :: utf8 >> = letter, count} <- letters, letter_count <- 1..count do %>
      ["<%= letter_code %>_<%= letter_count %>","<%= letter %>"],
      <% end %>
    ]
  }
  window.PLAYER_TOKEN = "<%= player_token %>";
</script>
